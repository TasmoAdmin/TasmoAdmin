{
    "name": "tasmoadmin/tasmoadmin",
    "type": "project",
    "license": "GPL3",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-zip": "*",
        "erusev/parsedown": "^1.7",
        "philipp15b/php-i18n": "^4.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/filesystem": "^5.4",
        "composer/semver": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "TasmoAdmin\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.8",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\TasmoAdmin\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4.3"
        }
    }
}
